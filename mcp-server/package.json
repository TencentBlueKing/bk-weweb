  "author": "BlueKing",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "express": "^4.22.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}